{
  "name": "mettingroom-reservation",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "antd": "^5.16.4",
    "core-fe": "^1.37.2",
    "lodash": "^4.17.21",
    "qs": "^6.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "test": "",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "no-var": "error",
      "prefer-const": "error",
      "semi": "error",
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "eol-last": "error",
      "require-yield": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "endOfLine": "auto",
    "printWidth": 100
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.24.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.17.0",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20.12.8",
    "@types/qs": "^6.9.15",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "mockjs": "^1.1.0",
    "prettier": "3.2.5",
    "typescript": "^5.0.0",
    "vite": "^5.2.0"
  }
}
